OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
PORT=3001

# CORS Configuration
# Comma-separated list of allowed origins
# Production example: https://aethermind-page.vercel.app,https://aethermind-agent-os-dashboard.vercel.app
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:5173

# Database (optional, uses InMemoryStore if not set)
DATABASE_URL=postgresql://user:password@localhost:5432/aethermind

# Redis (optional, for caching and queues)
REDIS_URL=redis://localhost:6379

# API Key for production (generate with: pnpm generate-api-key)
API_KEY_HASH=

# Node Environment
NODE_ENV=development

# JWT Secret (generate with: openssl rand -base64 32)
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PRO_PRICE_ID=price_...
STRIPE_ENTERPRISE_PRICE_ID=price_...

# Email Service Configuration
# Option 1: SendGrid (recommended for production)
SENDGRID_API_KEY=SG...
FROM_EMAIL=noreply@aethermind.com

# Option 2: SMTP via Nodemailer (good for development)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password
# FROM_EMAIL=noreply@aethermind.com

# Frontend URL (for email verification and reset links)
FRONTEND_URL=http://localhost:3000
