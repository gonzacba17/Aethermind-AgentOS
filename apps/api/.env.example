OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
PORT=3001

# CORS Configuration
# Comma-separated list of allowed origins
# Production example: https://aethermind-page.vercel.app,https://aethermind-agent-os-dashboard.vercel.app
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:5173

# Database (optional, uses InMemoryStore if not set)
DATABASE_URL=postgresql://user:password@localhost:5432/aethermind

# Redis (optional, for caching and queues)
REDIS_URL=redis://localhost:6379

# API Key for production (generate with: pnpm generate-api-key)
API_KEY_HASH=

# Node Environment
NODE_ENV=development
